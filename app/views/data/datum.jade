extends ../layout
block vars
	-currentPage = "Data";
block content
	-console.log(data);
	-title = (typeof data !== undefined) ? "Edit Event" : "Add Event"
	h1=title
block scripts
	-if(typeof data !== undefined)
		script.
			window.eventData = !{data};
	script(src='/assets/js/app/services/eventService.js')
	script(src='/assets/js/app/controllers/eventController.js')
